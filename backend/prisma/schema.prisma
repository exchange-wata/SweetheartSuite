datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "debian-openssl-1.1.x"]
}

model User {
  id    Int @default(autoincrement()) @id
  name  String
  mailaddress String  @unique
  createdAt DateTime
  updatedAt DateTime
  deletedAt DateTime?
}
